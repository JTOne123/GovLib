{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Unit Tests",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "Build Solution",
			"program": "dotnet",
			"args": ["vstest", "${workspaceRoot}/.vscode/build/Gov.NET.Tests/bin/Debug/netcoreapp2.0/Gov.NET.Tests.dll"],
			"console": "internalConsole",
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}
